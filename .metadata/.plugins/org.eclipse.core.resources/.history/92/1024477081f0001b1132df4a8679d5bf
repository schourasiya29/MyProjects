package com.sac.practice;

public class Test2 {

	public static void main(String[] args) {
		int arr[] = new int[] { 6, 3, 23, 19, 22, 11, 2, 1 };

		for (int i = 0; i <= arr.length; i++) {

			for (int j = 1; j <= arr.length-1; j++) {
				int temp = 0;
				if (arr[i] > arr[j]) {
					temp = arr[i];
					arr[i] = arr[j];
					arr[j] = temp;
				}
				System.out.print(arr[i]);
			}
		}
	}

}
