
public class Test {

	public static void main(String[] args) {
		System.out.println("Hello!");
		int[] arr = new int []{6,2,3,45,33,22,11,0,9,10};
		int temp=0;
		for (int i=0; i<arr.length;i++){
			for(int j=i;j<arr.length; j++){
				if (arr[i]<arr[j]){
					temp=arr[i];
					arr[i]=arr[j];
					arr[j]=temp;
				}
			}
		}
		
		for (int a:arr){
			System.out.println(a);
		}
	}

}
